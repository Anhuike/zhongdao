image {
  display: block;
  width: 100%;
  height: 100%;
}
.df {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
page {
  font-size: 24rpx;
  color: #464646;
  line-height: 35rpx;
  padding-bottom: 80px;
  background: #f7f7f7;
  font-family: '\5FAE\8F6F\96C5\9ED1', sans-serif, serif;
}
.bgja {
  background-image: linear-gradient(right, #00CEC1, #00B7B8);
  background-image: -webkit-linear-gradient(right, #00CEC1, #00B7B8);
}
.zdTabBar {
  position: fixed;
  left: 0;
  bottom: 0;
  right: 0;
  z-index: 20;
  background: url(data:image/png;base64,);
  background-size: 100% 100% ;
  padding-top: 30px ;
}
.zdTabBar .ul {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.zdTabBar .ul .li {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  padding: 5px 0;
  position: relative;
}
.zdTabBar .ul .li .img {
  width: 25px;
  height: 25px;
  overflow: hidden;
  margin: 0 auto;
}
.zdTabBar .ul .li .ic {
  width: 40px;
  height: 40px;
  background-image: linear-gradient(right, #00CEC1, #00B7B8);
  background-image: -webkit-linear-gradient(right, #00CEC1, #00B7B8);
  border-radius: 50%;
  text-align: center;
  font-size: 20px;
  line-height: 40px;
  font-weight: bold;
  color: #ffffff;
  margin: -20px auto 5px;
}
.zdTabBar .ul .li .p {
  margin-top: 2px;
  text-align: center;
  font-size: 10px;
  line-height: 18px;
}
.zdTabBar .ul .li.cur .p {
  color: #ec3851;
}

